{
  "name": "limpia-brew",
  "version": "1.0.1",
  "description": "Recursively remove brew formulae and its dependencies",
  "license": "BSD-3-Clause",
  "keywords": [
    "homebrew",
    "brew",
    "uninstall"
  ],
  "author": {
    "name": "Carlos Precioso",
    "email": "carlos@carlosprecioso.com",
    "homepage": "https://carlosprecioso.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cprecioso/limpia-brew.git"
  },
  "bugs": {
    "url": "https://gitlab.com/cprecioso/limpia-brew/issues"
  },
  "homepage": "https://gitlab.com/cprecioso/limpia-brew#README",
  "main": "lib/index.js",
  "bin": "bin.js",
